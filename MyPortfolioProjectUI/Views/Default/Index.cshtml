@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_DefaultHeadComponentPartial")
<body data-spy="scroll" data-target=".site-navbar-target" data-offset="300">
    @await Component.InvokeAsync("_DefaultNavBarComponentPartial")
    @await Component.InvokeAsync("_DefaultSliderComponentPartial")
    @await Component.InvokeAsync("_DefaultAboutMeComponentPartial")
    @await Component.InvokeAsync("_DefaultResumeComponentPartial")
    @await Component.InvokeAsync("_DefaultUseTechnologyComponentPartial")
    @await Component.InvokeAsync("_DefaultMySkillsComponentPartial")
    @await Component.InvokeAsync("_DefaultMyProjectComponentPartial")
    @await Component.InvokeAsync("_DefaultContactComponentPartial")
    @await Component.InvokeAsync("_DefaultFooterComponentPartial")
    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>
    @await Component.InvokeAsync("_DefaultScriptComponentPartial")
</body>
</html>